class Solution {
public:
    vector<int> intersection(vector<int>& nums1, vector<int>& nums2) {
        std::set<int> set_nums2{ nums2.begin(), nums2.end() };

        set<int> ans;

        for (auto num : nums1) {
            if (set_nums2.count(num)) {
                ans.insert(num);
            }
        }

        return {ans.begin(), ans.end()};
    }
};
